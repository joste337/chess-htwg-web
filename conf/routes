GET     /                           controllers.MainController.index
GET     /chess                      controllers.MainController.chess
POST    /chess/new                  controllers.MainController.newGrid
GET     /chess/move                 controllers.MainController.move(fromRow: Int, fromCol: Int, toRow: Int, toCol: Int)
POST    /chess/save                 controllers.MainController.save
GET     /chess/load                 controllers.MainController.load

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
